{
  "name": "avian-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx database/seeds/development.ts",
    "test:firewall-rollback": "tsx scripts/test-firewall-rollback.ts",
    "generate-firewall-key": "tsx scripts/generate-firewall-encryption-key.ts",
    "docker:build": "docker build -t avian-platform:latest .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "k8s:deploy": "./scripts/deploy.sh staging",
    "k8s:prod": "./scripts/deploy.sh production",
    "deploy:beta": "./scripts/deploy-beta.sh",
    "deploy:hotfix": "./scripts/deploy-hotfix.sh",
    "rollback": "./scripts/rollback.sh",
    "feature:enable": "./scripts/feature-flag.sh enable",
    "feature:disable": "./scripts/feature-flag.sh disable",
    "feature:list": "./scripts/feature-flag.sh list",
    "aws:setup": "./scripts/aws-setup.sh",
    "aws:deploy": "./scripts/deploy-aws.sh",
    "aws:amplify": "./scripts/deploy-aws.sh amplify",
    "aws:ecs": "./scripts/deploy-aws.sh ecs",
    "deploy:hipaa": "./scripts/deploy-hipaa-compliant.sh",
    "backup:hipaa": "./scripts/backup-hipaa.sh",
    "backup": "./scripts/backup.sh",
    "health": "curl -f http://localhost:3000/api/health/live",
    "team:sync": "./scripts/team-sync.sh",
    "team:status": "git status && git log --oneline -5",
    "team:setup": "./scripts/auto-setup.sh",
    "setup": "./scripts/setup/setup.sh",
    "test:local": "./scripts/testing/test-local.sh",
    "test:production": "./scripts/testing/test-production.sh",
    "pre-commit": "npm run lint && npm run format:check && npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.958.0",
