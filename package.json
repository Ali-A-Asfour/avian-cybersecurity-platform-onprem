    "@aws-sdk/client-secrets-manager": "^3.947.0",
    "@aws-sdk/client-ssm": "^3.958.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@aws-sdk/util-dynamodb": "^3.958.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.44.7",
    "express-rate-limit": "^8.2.1",
    "imap": "^0.8.17",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.548.0",
    "mailparser": "^3.9.0",
    "next": "^16.1.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "redis": "^5.9.0",
    "swr": "^2.3.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
